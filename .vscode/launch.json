{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Unit tests",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/gulp/bin/gulp.js",
			"stopOnEntry": false,
			"args": [
                "unit-tests"
            ],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development",
				"SKIP_COVERAGE": "True"
			},
			"externalConsole": false,
			"sourceMaps": true,
			"outDir": null
		}
	]
}